{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/shared/infra/http/server.js",
  "private": true,
  "scripts": {
    "lint": "eslint --ext ./src/**/*.ts ./src/**/*.ts",
    "lint:fix": "eslint --ext .ts --fix ./src/**/*.ts",
    "format": "prettier --check --write **/*.ts",
    "clear": "sh ./scripts/clear_db.sh",
    "build": "swc src -d dist",
    "dev": "NODE_ENV=development nodemon --ext ts --exec node --inspect -r @swc/register src/shared/infra/http/server.ts",
    "start": "node dist/shared/infra/http/server.js",
    "migrate": "sh ./scripts/migrate_dev.sh",
    "generate": "sh ./scripts/generate.sh",
    "test": "jest",
    "test:unit": "jest src/__tests__/unit/**/*.test.ts",
    "test:e2e": "jest src/__tests__/integration/**/*.e2e.test.ts",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@prisma/client": "5.3.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.1",
    "google-auth-library": "^9.0.0",
    "helmet": "^7.0.0",
    "jest-extended": "^4.0.1",
    "morgan": "^1.10.0",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.2",
    "ts-node": "^10.9.1",
    "tsyringe": "^4.8.0",
    "winston": "^3.11.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.7",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.78",
    "@swc/jest": "^0.2.29",
    "@swc/register": "^0.1.10",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "3.0.5",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "prisma": "5.3.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}